{
  "name": "playwright-visual-builder",
  "version": "1.0.0",
  "description": "Visual test builder for Playwright E2E tests",
  "private": true,
  "workspaces": [
    "client",
    "server",
    "shared"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm run dev -w server",
    "dev:client": "npm run dev -w client",
    "build": "npm run build -w shared && npm run build -w server && npm run build -w client",
    "start": "npm run start -w server"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "js-yaml": "^4.1.0",
    "multer": "^2.0.2",
    "uuid": "^11.1.0"
  }
}
